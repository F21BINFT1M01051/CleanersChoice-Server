{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/create-subscription",
      "dest": "/api/create-subscription.js"
    },
    {
      "src": "/api/confirm-subscription",
      "dest": "/api/confirm-subscription.js"
    },
    {
      "src": "/api/cancel-subscription",
      "dest": "/api/cancel-subscription.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
     {
      "src": "/api/send-notification",
      "dest": "/api/send-notification.js"
    },
     {
      "src": "/api/apple-validate",
      "dest": "/api/apple-validate.js"
    },
     {
      "src": "/api/apple-webhook",
      "dest": "/api/apple-webhook.js"
    }
  ]
}
